{
  "name": "Little Yeti",
  "type": "dark",
  "colors": {
    "editor.background": "#383838",
    "editor.foreground": "#bbbbbb",
    "editor.lineHighlightBackground": "#2b2b2b",
    "editorCursor.foreground": "#bbbbbb",
    "editorLineNumber.foreground": "#666666",
    "sideBar.background": "#222222",
    "sideBar.foreground": "#bbbbbb",
    "sideBarTitle.foreground": "#bbbbbb",
    "statusBar.background": "#2b2b2b",
    "statusBar.foreground": "#bbbbbb"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#666666"
      }
    },
    {
      "name": "Doc Comment",
      "scope": [
        "comment.line.documentation"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#47B5E7"
      }
    },
    {
      "name": "Declaration Keywords",
      "scope": [
        "keyword.default",
        "keyword.operator.new",
        "keyword.storage",
        "storage.modifier",
        "storage.type",
        "storage.type.function"
      ],
      "settings": {
        "foreground": "#e06c75"
      }
    },
    {
      "name": "Builtin",
      "scope": [
        "support.function.builtin"
      ],
      "settings": {
        "foreground": "#00ddad"
      }
    },
    {
      "name": "String",
      "scope": [
        "string.quoted"
      ],
      "settings": {
        "foreground": "#e5c07b"
      }
    },
    {
      "name": "Number/Literals",
      "scope": [
        "constant.numeric",
        "constant.language",
        "keyword.constant.bool"
      ],
      "settings": {
        "foreground": "#b77ee0"
      }
    },
    {
      "name": "Type",
      "scope": [
        "entity.name.type",
        "keyword.type",
        "support.type.primitive.ts"
      ],
      "settings": {
        "foreground": "#00afaf"
      }
    },
    {
      "name": "Type",
      "scope": [
        "keyword.operator",
        "meta.type.annotation"
      ],
      "settings": {
        "foreground": "#bbbbbb"
      }
    },
    {
      "name": "Control Flow",
      "scope": [
        "keyword.control",
        "keyword.operator.expression.is.ts"
      ],
      "settings": {
        "foreground": "#47B5E7"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": [
        "markup.heading.markdown"
      ],
      "settings": {
        "foreground": "#e06c75",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold.markdown"
      ],
      "settings": {
        "foreground": "#00ddad",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": [
        "markup.italic.markdown"
      ],
      "settings": {
        "foreground": "#b77ee0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Link Text",
      "scope": [
        "meta.link.inline.markdown"
      ],
      "settings": {
        "foreground": "#47B5E7"
      }
    },
    {
      "name": "Markdown Link URL",
      "scope": [
        "markup.underline.link.markdown"
      ],
      "settings": {
        "foreground": "#47B5E7",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Inline Code Block",
      "scope": [
        "markup.inline.raw.string.markdown"
      ],
      "settings": {
        "background": "#00FF00ff",
        "foreground": "#e06c75"
      }
    },
    {
      "name": "JSON key",
      "scope": [
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#47B5E7"
      }
    }
  ]
}